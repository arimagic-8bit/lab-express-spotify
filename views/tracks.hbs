<div>

    {{#each track}}
        <table>
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Listen</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>{{name}}</td>
                    <td>
                        <audio controls src="{{preview_url}}"></audio>
                    </td>
                </tr>
            </tbody>
            
        </table>
    {{/each}}

</div>